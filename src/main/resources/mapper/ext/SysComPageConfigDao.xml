<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tools.st.mapper.SysComPageConfigDao">

  <select id="findByPageId" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List"/>
    from sys_com_page_config
    where enableflg=1 and page_id=#{pageId}

  </select>

  <select id="findPages" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List"/>
    from sys_com_page_config
  </select>
</mapper>